{
  "name": "zjadlempl",
  "version": "0.0.0",
  "scripts": {
    "ng": "node -r dotenv/config ./node_modules/@angular/cli/bin/ng",
    "start": "node -r dotenv/config ./node_modules/@angular/cli/bin/ng serve",
    "build": "node -r dotenv/config ./node_modules/@angular/cli/bin/ng build",
    "watch": "node -r dotenv/config ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "node -r dotenv/config ./node_modules/@angular/cli/bin/ng test",
    "build-tailwind": "tailwindcss -i ./src/styles.css -o ./src/output.css --watch",
    "start-with-tailwind": "concurrently \"npm run start\" \"npm run build-tailwind\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@supabase/supabase-js": "^2.43.4",
    "daisyui": "^4.12.2",
    "ngx-sonner": "^1.0.0",
    "rxjs": "~7.8.0",
    "uuidv4": "^6.2.13",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.2",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.0",
    "@ngx-env/builder": "^18.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.14.2",
    "dotenv": "^16.4.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "~5.4.2"
  }
}
